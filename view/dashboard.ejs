
<!-- Menyertakan file header.ejs dengan variabel lokal -->
<%- include('header.ejs', { pageTitle: 'Halaman Dashboard' }) %>

<!-- Isi utama untuk halaman dashboard -->
<main class="container-fluid">
  <div class="row">


    <!-- Konten utama -->
    <div class="col-sm-10 ml-sm-auto col-md-9 ml-md-auto ml-lg-auto pt-3">
      <h2>Dashboard</h2>
      <div class="card">
        <div class="card-body">
          <p>ID: <%= user.id %></p>
          <p>Name: <%= user.name %></p>
          <p>Email: <%= user.email %></p>
        </div>
      </div>
    </div>
  </div>
</main>

<!-- Menyertakan file footer.ejs -->
<%- include('footer.ejs') %>